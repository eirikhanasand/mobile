{
  "expo": {
    "name": "bubbles",
    "slug": "bubbles",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
        "image": "./public/assets/images/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
    },
    "ios": {
        "supportsTablet": true,
        "bundleIdentifier": "com.bubbles.bubbles"
    },
    "android": {
        "adaptiveIcon": {
            "foregroundImage": "./public/assets/images/adaptive-icon.png",
            "backgroundColor": "#ffffff"
        },
        "package": "com.bubbles.bubbles"
    },
    "web": {
        "bundler": "metro",
        "output": "static",
        "favicon": "./public/assets/images/favicon.png"
    },
    "plugins": [
        [
            "expo-router",
            {
                "root": "./src/app"
            }
        ]
    ],
    "experiments": {
        "typedRoutes": true
    }
  }
}
